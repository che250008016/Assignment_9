#include <iostream>
using namespace std;


struct Item {
    int id;      
    float cost;  
};

int main() {
    
    Item items[6] = {
        {101, 45.50}, 
        {102, 120.75}, 
        {103, 30.25}, 
        {104, 75.00}, 
        {105, 55.30}, 
        {106, 49.99},
    };


    cout << "Items with cost greater than 50.00 are:" << endl;
    for (int i = 0; i < 6; i++) {
        if (items[i].cost > 50.00) {
            cout << "Item ID: " << items[i].id << ", Cost: " << items[i].cost << endl;
        }
    }

    return 0;
}
